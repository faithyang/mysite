<div class="upload_file">
	<%= form_tag({action: :upload_file}, multipart: true) do %>
	  <%= file_field_tag 'picture' %>
		<%= submit_tag 'upload!' %>
	<% end %>
</div>

<p>Uploaded files:</p>
<div class="file_list">
	<table class='bigtable'>
		<% @uploaded_files.each do |f| %>
		<tr>
			<td><%= f %></td>
		</tr>
		<% end %>
	
	
	</table>
</div>